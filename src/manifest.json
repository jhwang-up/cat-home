{
  "package": "com.application.cathome",
  "name": "cat-home",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.share"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "Launch",
    "pages": {
      "Launch": {
        "component": "index"
      },
      "Index": {
        "component": "index"
      },
      "DemoDetail": {
        "component": "index"
      },
      "About": {
        "component": "index"
      }
    },
    "widgets": {
      "CardDemo": {
        "name": "CardDemo",
        "description": "快应用卡片展示",
        "component": "index",
        "path": "/CardDemo",
        "features": []
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "statusBarImmersive": true,
    "statusBarBackgroundColor ": "#ffffff",
    "menu": true,
    "pages": {
      "Index": {
        "menu": false,
        "titleBarBackgroundColor": "#ffffff"
      },
      "DemoDetail": {
        "titleBarText": "详情页",
        "menu": true
      },
      "Launch": {
        "menu": false,
        "titleBarBackgroundColor": "#FC968D"
      }
    }
  }
}